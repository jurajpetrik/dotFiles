[color]
    branch = auto
    diff = auto
    grep = auto
    interactive = auto
    status = auto
	ui = true
[alias]
    a = add
    s = status
    st = status
    c = commit
    cm = commit -m
    d = diff
    lg = log --oneline --decorate
    co = checkout
    cb = checkout -b
    cob = checkout -b
    pom = push origin master
    poh = push origin HEAD
    pl = pull
    mg = merge --no-ff
    ft = fetch
    f = fetch
    ours = checkout --ours
    theirs = checkout --theirs
[user]
	email = jup@debitoor.com
	name = Juraj Petrik
[core]
	preloadindex = true
	fscache = true
	whitespace = cr-at-eol
[gc]
	auto = 0
[push]
	default = simple
[colors]
	ui = true
