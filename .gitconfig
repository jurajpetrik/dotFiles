[color]
  branch = auto
  diff = auto
  grep = auto
  interactive = auto
  status = auto
  ui = true
[alias]
  a = add
  s = status
  c = commit
  cm = commit -m
  d = diff
  dc = diff --cached
  co = checkout
  cb = checkout -b
  cob = checkout -b
  bdel = branch -D
  bd = branch -D
  unstage = reset HEAD
  amend = commit --amend --no-edit
  reword = commit --amend
  pop = stash pop
  touched = "!f() { BASE_BRANCH=${1:-develop}; printf \"Files changed since branched out from $BASE_BRANCH:\n\n\"; git diff HEAD $(git merge-base HEAD  $BASE_BRANCH) --name-only; }; f"
  ibranch = "!f() { git checkout $(git branch | fzf); }; f"
  bhist = "!f() { pwd=$PWD; cd "$(git rev-parse --show-toplevel)"; cut -f2- .git/logs/HEAD | awk '$1==\"checkout:\" { print $NF }' | head -10; cd $pwd; }; f"
  bh    = "!f() { pwd=$PWD; cd "$(git rev-parse --show-toplevel)"; cut -f2- .git/logs/HEAD | awk '$1==\"checkout:\" { print $NF }' | head -10; cd $pwd; }; f"
  fork = "!f() { git checkout -b $(git symbolic-ref --short HEAD)-$1; }; f"
  root = rev-parse --show-toplevel
  ps = push
  psf = push --force
  pl = pull
  f = fetch
  l = log
  file-log = log --follow -p --
[user]
  email = juraj.petrik@outreach.io
  name = Juraj Petrik
[core]
  preloadindex = true
  fscache = true
  whitespace = cr-at-eol
  editor = vim
  excludesfile = ~/.global_ignore
[gc]
  auto = 0
[push]
  default = current
[colors]
  ui = true
[init]
  templatedir = ~/.git_template
[credential]
	helper = osxkeychain
[github]
	user = jurajpetrik
[url "git@github.com:"]
	insteadOf = https://github.com/

